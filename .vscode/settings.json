{
  "eslint.format.enable": true,
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[scss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint",
  },
  "[css]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint",
  },
  "files.eol": "\n",
  "eslint.run": "onType",
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "eslint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "vue.inlayHints.optionsWrapper": false,
  "stylelint.packageManager": "pnpm",
  "stylelint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelint.snippet": [
    "css",
    "postcss",
    "scss",
    "vue"
  ],
  "stylelint.validate": [
    "css",
    "postcss",
    "scss",
    "vue"
  ]
}